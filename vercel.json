{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "^/assets/(.*)",
      "headers": {
        "Content-Type": "application/javascript"
      },
      "dest": "/assets/$1"
    },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ]
}
